      // Summary: Formats a text to highlight a set of characters
      // Syntax:
      //[ <Result> = ] _RibbonSearch_HighlightText (<sInitialText> is string, <sSoughtTerm> is string, <sReferenceControl> is string)
      //
      // Parameters:
      //	sInitialText (ANSI string): Initial text
      //	sSoughtTerm (ANSI string): Sought term that must be highlighted in the initial text
      //	sReferenceControl (ANSI string): Reference control to retrieve the initial font
      // Return value:
      // 	ANSI string: Formatted text
      //// Automatic process of errors: RESULT Empty string "
      //
      
      PROCEDURE PRIVATE _RibbonSearch_HighlightText(LOCAL sInitialText is string, LOCAL sSearchTerm is string, LOCAL sReferenceControl is string)
      
      sFinalText	is string
      nStartPos		is int
      
      
      // Stores the initial text
      sFinalText = sInitialText
      
      // Finds each occurrence of the term
      nStartPos = Position(sFinalText, sSearchTerm, 1, IgnoreCase)
      IF nStartPos > 0
      	
      	// Formats this text section
      	sFinalText = RTFSelection(sFinalText, rtfBackgroundColor, LightYellow, nStartPos, Length(sSearchTerm))
      	
      END
      
      // Applies the font (name and size) of the reference control
      sFinalText = RTFSelection(sFinalText, rtfFontName, 		{sReferenceControl, indControl}..FontName, 	1, Length(sFinalText))
      sFinalText = RTFSelection(sFinalText, rtfFontSize, 	{sReferenceControl, indControl}..FontSize, 	1, Length(sFinalText))
      
      
      // Formatted text
      RESULT sFinalText
