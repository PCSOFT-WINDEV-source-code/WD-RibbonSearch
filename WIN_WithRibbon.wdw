   -
     name : _RibbonSearch_DefinePaneImage
     internal_properties : 
     procedure_id : 1760458969530245971
     type_code : 14
     code : |1+
      // Summary: Creates a pane image from the initial image by applying a color
      // Syntax:
      //[ <Result> = ] _RibbonSearch_DefinePaneImage (<nColor> is int)
      //
      // Parameters:
      //	nColor (integer): Color to apply
      // Return value:
      // 	ANSI string: Path of result image file
      //// Automatic process of errors: RESULT Empty string "
      //
      
      PROCEDURE PRIVATE _RibbonSearch_DefinePaneImage(LOCAL nColor is int) : string
      
      // Defines the temporary image file
      sTemporaryImage is string = fTempFile+".png"
      
      // Applies the color to the background of the final image
      IMG_ImageFinalPane..BackgroundColor = nColor
      
      // Copies the image of the pane into the image control
      dCopyBlt(IMG_PaneImage, IMG_ImageFinalPane, copySrcAnd)
      
      // Stores the drawing in a temporary file
      dSaveImagePNG(IMG_ImageFinalPane, sTemporaryImage)
      
      // End of drawing
      dEndDrawing(IMG_ImageFinalPane)
      
      
      // Path of result file
      RESULT sTemporaryImage
     type : 458752
