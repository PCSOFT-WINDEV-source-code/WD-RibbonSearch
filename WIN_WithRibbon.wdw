      // Summary: Displays the results of a search 
      // Syntax:
      //_RibbonSearch_DisplayResult ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //// Automatic procedure:
      // The procedure is run manually during a call in the code, with a delay set to 1 hundredths of a second
      // 
      
      PROCEDURE _RibbonSearch_DisplayResult()
      
      sFormattedCaption 	is string
      sFormattedTooltip 		is string
      
      
      // Deletes all the controls of the previous search
      DeleteAll(garrRibbonElementsSearch)
      
      // Clears the looper
      LooperDeleteAll(LOOP_ResultSearch)
      
      // For each ribbon control
      FOR ALL ELEMENT stAControl of garrRibbonElements
      	
      	// If the term of the search is found for this control
      	IF Position(stAControl.sSoughtText, EDT_FindRibbonCommand, 1, IgnoreCase) > 0 THEN
      		
      		// Only the visible elements must be taken into account
      		IF _RibbonSearch_ElementIsVisible(stAControl) THEN
      			
      			// Stores this control as search result
      			Add(garrRibbonElementsSearch, stAControl)
      			
      			// Formats the caption (to highlight the sought term)
      			sFormattedCaption = _RibbonSearch_HighlightText(stAControl.sCaption, EDT_FindRibbonCommand, STC_ControlCaption..FullName)
      			
      			// Formats the tooltip (to highlight the sought term)
      			sFormattedTooltip = _RibbonSearch_HighlightText(stAControl.sToolTip, EDT_FindRibbonCommand, STC_ControlTooltip..FullName)
      			
      			// Adds a row into the looper
      			LooperAddLine(LOOP_ResultSearch, sFormattedCaption, sFormattedTooltip, stAControl.sRibbonAccessPath)
      			
      		END
      		
      	END
      	
      END
      
      // Don't select anything by default
      LooperSelectMinus(LOOP_ResultSearch)
      
      // Modifies the height according to the number of results
      nNbRow is int 			= Min(LOOP_ResultSearch..Count, 5)
      // The height of the Looper control corresponds to the height of a row + 1 (for the separation line) * the number of rows to display (up to 5)
      LOOP_ResultSearch..Height 	= (LOOP_ResultSearch..LineHeight+1) * nNbRow
      // The height of the Image control corresponds to its initial height + the height occupied by the additional looper rows
      IMG_BackgroundSearch..Height 		= IMG_BackgroundSearch..InitialHeight + (LOOP_ResultSearch..LineHeight+1) * (nNbRow-1)
