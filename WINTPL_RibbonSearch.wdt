      // Summary: Loads all the ribbon controls
      // Syntax:
      //[ <Result> = ] _RibbonSearch_Initialize ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	boolean: True if the load is correct, False otherwise
      //
      // Automatic process of errors: RETURN FALSE
      //
      
      procedure private _RibbonSearch_Initialize() : boolean
      
      stAnElement 			is STRibbonElement
      sGroupName 		is string
      sControlName				is string
      sPaneCaption			is string
      sGroupCaption	is string
      sControlCaption			is string
      
      
      // The name of the Ribbon control must be correct
      if ControlExist(gsNameRibbonControl) = False then
      	ErrorBuild("The name of the ribbon control (""%1"") is invalid." +CR+ "You must fill the gsNameRibbonControl variable of the window template", gsNameRibbonControl)
      	result False
      end
      
      // Clears the previous storage
      DeleteAll(garrRibbonElements)
      DeleteAll(garrRibbonElementsSearch)
      
      // Browses the ribbon panes
      for nPaneSubscript = 1 _to_ {gsNameRibbonControl, indControl}..Count
      	
      	// Stores the caption of the pane (or the subscript of the pane if the caption is empty)
      	sPaneCaption = ({gsNameRibbonControl, indControl}[nPaneSubscript]..Caption <> "" ? {gsNameRibbonControl, indControl}[nPaneSubscript]..Caption else nPaneSubscript)
      	
      	// Stores the pane as a ribbon control
      	VariableReset(stAnElement)
      	stAnElement.sFullControlName 	= nPaneSubscript
      	stAnElement.sCaption 			= sPaneCaption
      	stAnElement.sToolTip 			= {gsNameRibbonControl, indControl}[nPaneSubscript]..ToolTip
      	stAnElement.sSoughtText 	= stAnElement.sCaption +" "+ stAnElement.sToolTip
      	stAnElement.sRibbonAccessPath 	= StringBuild("Ribbon >>> ""%1"" pane", sPaneCaption)
      	stAnElement.nPaneSubscript 		= nPaneSubscript
      	stAnElement.eType				= eltPane
      	Add(garrRibbonElements, stAnElement)
      	
      	// Browses the pane groups
      	sGroupName = EnumSubElement(gsNameRibbonControl+"."+nPaneSubscript, enumFirst)
      	while sGroupName <> ""
      		
      		// Is it a group ?
      		if {sGroupName, indControl}..Type <> typRibbonGroup then continue
      		
      		// Stores the caption of the group (or the name of the group if the caption is empty)
      		sGroupCaption = ({sGroupName, indControl}..Caption <> "" ? {sGroupName, indControl}..Caption else {sGroupName, indControl}..Name)
      		
      		// Stores the group as a ribbon control
      		VariableReset(stAnElement)
      		stAnElement.sFullControlName 	= {sGroupName, indControl}..FullName
      		stAnElement.sCaption 			= sGroupCaption
      		stAnElement.sToolTip 			= {sGroupName, indControl}..ToolTip
      		stAnElement.sSoughtText 	= stAnElement.sCaption +" "+ stAnElement.sToolTip
      		stAnElement.sRibbonAccessPath 	= StringBuild("Ribbon >>> ""%1"" pane >>> ""%2"" group", sPaneCaption, sGroupCaption)
      		stAnElement.nPaneSubscript 		= nPaneSubscript
      		stAnElement.eType				= eltGroup
      		Add(garrRibbonElements, stAnElement)
      		
      		// Browses the group controls
      		sControlName = EnumSubElement(gsNameRibbonControl+"."+nPaneSubscript+"."+sGroupName, enumFirst)
      		while sControlName <> ""
      			
      			// Stores the caption of the control (or the name of the control if the caption is empty)
      			sControlCaption = ({sControlName, indControl}..Caption <> "" ? {sControlName, indControl}..Caption else {sControlName, indControl}..Name)
      			
      			// Stores the control
      			VariableReset(stAnElement)
      			stAnElement.sFullControlName 	= {sControlName, indControl}..FullName
      			stAnElement.sCaption 			= {sControlName, indControl}..Caption
      			stAnElement.sToolTip 			= {sControlName, indControl}..ToolTip
      			stAnElement.sSoughtText 	= stAnElement.sCaption +" "+ stAnElement.sToolTip
      			stAnElement.sRibbonAccessPath 	= StringBuild("Ribbon >>> ""%1"" pane >>> ""%2"" group >> ""%3"" control", sPaneCaption, sGroupCaption, sControlCaption)
      			stAnElement.eType				= eltControl
      			stAnElement.nPaneSubscript 		= nPaneSubscript
      			stAnElement.sParentGroup	= {sGroupName, indControl}..FullName
      			Add(garrRibbonElements, stAnElement)
      			
      			// Next control
      			sControlName = EnumSubElement(gsNameRibbonControl+"."+nPaneSubscript+"."+sGroupName)	
      			
      		end
      		
      		CONTINUE:
      		// Next group
      		sGroupName = EnumSubElement(gsNameRibbonControl+"."+nPaneSubscript)	
      		
      	end
      	
      end
      
      //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // Deletes the CR characters from the captions used in the looper (replaced by a space)
      FOR ALL ELEMENT stAnElement of garrRibbonElements
      	stAnElement.sToolTip 		= StringFormat(Replace(stAnElement.sToolTip, CR, " "), 			ccIgnoreSpace)
      	stAnElement.sCaption 			= StringFormat(Replace(stAnElement.sCaption, CR, " "), 			ccIgnoreSpace)
      	stAnElement.sRibbonAccessPath = StringFormat(Replace(stAnElement.sRibbonAccessPath, CR, " "), 	ccIgnoreSpace)
      end
      
      //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // Positions the search controls in height according to the ribbon 
      IMG_BackgroundSearch..Y 			= {gsNameRibbonControl, indControl}..Y + {gsNameRibbonControl, indControl}..Height
      EDT_FindRibbonCommand..Y 	= IMG_BackgroundSearch..Y + 2
      BTN_CloseRibbonSearch..Y 		= IMG_BackgroundSearch..Y + 2
      LOOP_ResultSearch..Y 		= IMG_BackgroundSearch..Y + 27
      
      //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // Stores the image of the ribbon (file path)
      gsImageInitialPane 			= {gsNameRibbonControl, indControl}..Image
      // Loads in an Image variable
      MyImage is an Image 			= gsImageInitialPane
      // Displays the initial image in a control
      IMG_PaneImage 					= gsImageInitialPane
      // Positions the dimensions of the temporary controls
      // (these controls will be used to create pane images according to specific colors to highlight the pane during the search)
      IMG_PaneImage..Width 		= Max(1, MyImage ..Width)
      IMG_PaneImage..Height 		= Max(1, MyImage ..Height)
      IMG_ImageFinalPane..Width 	= Max(1, MyImage..Width)
      IMG_ImageFinalPane..Height 	= Max(1, MyImage..Height)
      
      
      // Load performed
      result True
