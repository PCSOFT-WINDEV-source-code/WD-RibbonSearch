     code : |1+
      
      EXTERN "WINCONST.WL"
      
      EElementType is enumeration
      	eltUnknown			// Unknown type of element
      	eltPane			// Pane
      	eltGroup		// Grouping
      	eltControl			// Control
      END
      
      // Structure that contains the information regarding the different elements (controls, groups, panes) of the ribbon
      STRibbonElement is Structure
      	sFullControlName 		is string						// Full name of the element (group/control= or pane number
      	sRibbonAccessPath 		is string						// Access path to the ribbon (pre-formatted text)
      	sCaption 				is string						// Caption of the element
      	sToolTip 				is string						// Tooltip of the element
      	sSoughtText 		is string						// Text used for the search (corresponds to the concatenation of the caption and tooltip)
      	nPaneSubscript 			is int						// Subscript of the control/group pane
      	sParentGroup		is string						// Full name of the group
      	eType					is EElementType					// Type of the element (pane, group or control)
      END
      
      // Variables of the window template
      gsImageInitialPane			is string						// Image initially used for the ribbon panes
      gsNameRibbonControl				is string						// Name of the ribbon control used (this variable must be initialized in the window that is using the template)
      garrRibbonElements 			is array of STRibbonElement	// Array that contains all the ribbon elements
      garrRibbonElementsSearch 		is array of STRibbonElement	// Array that contains the filtered elements
   -
