     name : RibbonSearch_Display
     internal_properties : CAAAAAgAAACO0YJO/DBdll2Gbdld8/ULoYk2WSJGl5dLaNpYAk627ju0DkmbL5l+w4tZk10IVFLkokMfmK+o9/Aj/hIGN9N9gsaIRiZzMGtbM8G0Ll76rnW3KnLTb309bZ8LRCdtT2plji2OvG5QOqpXJxjLrJjdCRKWji0m8gTXKZm/oBe7HZie
     procedure_id : 1760458982415805297
     type_code : 14
     group : 4
     code : |1+
      // Summary: Displays or hides the controls linked to the search in the ribbon
      // Syntax:
      //RibbonSearch_Display ([<bDisplay> is boolean])
      //
      // Parameters:
      //	bDisplay (boolean): True to display the elements, False to hide them. If the parameter is not specified, the current visibility is reversed
      // Return value:
      // 	None
      //
      PROCEDURE RibbonSearch_Display(LOCAL bDisplay is boolean = NOT GR_RibbonSearch..Visible)
      
      
      // If the controls must be displayed
      IF bDisplay THEN
      	
      	// Displays the search controls (background image, edit, looper, ...)
      	GR_RibbonSearch..Visible = True
      	
      	// Runs the search (by default, the edit control stores the last value entered)
      	ExecuteProcess(EDT_FindRibbonCommand, trtChange)
      	
      	// Resume the input
      	ReturnToCapture(EDT_FindRibbonCommand)
      	
      	
      ELSE // If the controls must be hidden
      	
      	// Hides the search controls (background image, edit, looper, ...)
      	GR_RibbonSearch..Visible 			= False
      	
      	// Hides the image and the superposition shape
      	IMG_Highlight_Background..Visible 	= False
      	SHA_Highlight_Border..Visible 	= False
      	
      	// Re-applies the initial pane image 
      	IF ControlExist(gsNameRibbonControl) THEN
      		{gsNameRibbonControl, indControl}..Image	= gsImageInitialPane
      	END
      	
      END
     type : 458752
   -
     name : _RibbonSearch_LeftClickWindow
     procedure_id : 1760458982415870833
     type_code : 14
     code : |1-
      // Summary: Procedure called during a left click on the window
      // Syntax:
      //_RibbonSearch_LeftClickWindow ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE PRIVATE _RibbonSearch_LeftClickWindow()
      
      // If the search group is visible
      IF GR_RibbonSearch..Visible THEN
      	
      	// If the click was performed outside the area dedicated to the search in the ribbon
      	IF MouseYPos(mpWindow) < IMG_BackgroundSearch..Y _OR_ MouseYPos(mpWindow) > IMG_BackgroundSearch..Y+IMG_BackgroundSearch..Height THEN
      		
      		// Hides the elements of the search (the application is going back to normal use)
      		RibbonSearch_Display(False)
      		
      	END
      	
      END
     type : 458752
