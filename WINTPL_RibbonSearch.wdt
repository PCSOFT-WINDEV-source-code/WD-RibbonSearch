      // Summary: Defines whether an element is visible or not
      // Syntax:
      //[ <Result> = ] _RibbonSearch_ElementIsVisible (<stAnElement> is STRibbonElement)
      //
      // Parameters:
      //	stAnElement (STRibbonElement): Full name of the element
      // Return value:
      // 	boolean: True if the element is visible, False otherwise
      //// Automatic process of errors: RETURN FALSE
      //
      
      PROCEDURE PRIVATE _RibbonSearch_ElementIsVisible(LOCAL stAnElement is STRibbonElement) : boolean
      
      // The pane must be visible regardless of the element type
      IF {gsNameRibbonControl, indControl}[stAnElement.nPaneSubscript]..Visible = False THEN RESULT False
      
      WHEN EXCEPTION IN
      	
      	// According to the type of the element
      	SWITCH stAnElement.eType
      		// Pane
      		CASE eltPane
      			// The case of the invisible pane is processed on the first code line
      			RESULT True
      			
      		// Grouping
      		CASE eltGroup			
      			// Returns	the visibility of the group 
      			RESULT {stAnElement.sFullControlName, indControl}..Visible
      			
      		// Control
      		CASE eltControl
      			// The group must be visible
      			IF {stAnElement.sParentGroup, indControl}..Visible = False THEN RESULT False
      			
      			// Returns	the visibility of the control 
      			RESULT {stAnElement.sFullControlName, indControl}..Visible
      			
      		// Unknown
      		OTHER CASE
      			dbgAssert(False, "Unknown type of element")
      			
      	END
      	
      	// If nothing is found, returns False
      	RESULT False
      	
      DO
      	RESULT False
      END
