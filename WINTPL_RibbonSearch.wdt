     name : _RibbonSearch_HighlightControl
     internal_properties : CAAAAAgAAABow7TPBEb1gRhYB+Em1DvOCTbOKiZhXcbaLNQRQep6e4wJq8rp9CCZUipGmI7oaV4aolajJlg1k5Ie3bYsLkRhubronEjo755wCCL7AwgnTH1yUjQUCdblo8Ary0arC7KiYfJSu9ygS1/aVRb/H2JP2K60tVPEz3R/NAN/5DT0aUmbQ7HC/WTRv6wONosPSl3vB+WYiwA=
     procedure_id : 1760458982415936369
     type_code : 14
     code : |1-
      // Summary: Highlights the control, group or pane of the selected search result
      // Syntax:
      //_RibbonSearch_HighlightControl (<stAnElement> is STRibbonElement)
      //
      // Parameters:
      //	stAnElement (STRibbonElement): Structure containing the information of the control to highlight
      
      // Return value:
      PROCEDURE PRIVATE _RibbonSearch_HighlightControl(stAnElement is STRibbonElement)
      
      // The Ribbon control must exist
      IF ControlExist(gsNameRibbonControl) = False THEN RETURN
      
      // Positions the ribbon on the pane
      {gsNameRibbonControl, indControl} = stAnElement.nPaneSubscript
      
      // Hides the controls that are used to highlight the controls and groups
      IMG_Highlight_Background..Visible 	= False
      SHA_Highlight_Border..Visible 	= False
      
      // If the result to highlight is a group or a control
      IF stAnElement.eType IN (eltControl, eltGroup) THEN
      	
      	// Overlays the highlighting image
      	IMG_Highlight_Background..Width 	= {stAnElement.sFullControlName, indControl}..Width
      	IMG_Highlight_Background..Height 	= {stAnElement.sFullControlName, indControl}..Height
      	IMG_Highlight_Background..X 			= {stAnElement.sFullControlName, indControl}..X
      	IMG_Highlight_Background..Y 			= {stAnElement.sFullControlName, indControl}..Y
      	
      	// Frames the control or group
      	SHA_Highlight_Border..Width 	= {stAnElement.sFullControlName, indControl}..Width+2
      	SHA_Highlight_Border..Height 	= {stAnElement.sFullControlName, indControl}..Height+2
      	SHA_Highlight_Border..X 		= {stAnElement.sFullControlName, indControl}..X-1
      	SHA_Highlight_Border..Y 		= {stAnElement.sFullControlName, indControl}..Y-1
      	
      	// Displays the controls that are used for the highlight (the image for the background and the shape for the border) 
      	IMG_Highlight_Background..Visible 	= True
      	SHA_Highlight_Border..Visible 	= True
      	
      END
      
      // Switches the image, the border and the background from yellow to red...
      FOR i = 0 _TO_ IMG_GradientRibbonSearch..Width-1
      	{gsNameRibbonControl, indControl}..Image 		= _RibbonSearch_DefinePaneImage(dPixelColor(IMG_GradientRibbonSearch, i, 5))
      	IMG_Highlight_Background..BackgroundColor	= dPixelColor(IMG_GradientRibbonSearch, i, 5)
      	SHA_Highlight_Border..Color		= dPixelColor(IMG_GradientRibbonSearch, i, 5)
      	Wait(0, waitMouseAndKeyboard)
      END
      
      // ... then from red to yellow
      FOR i = IMG_GradientRibbonSearch..Width-1 _TO_ 0 STEP -1
      	{gsNameRibbonControl, indControl}..Image 		= _RibbonSearch_DefinePaneImage(dPixelColor(IMG_GradientRibbonSearch, i, 5))
      	IMG_Highlight_Background..BackgroundColor	= dPixelColor(IMG_GradientRibbonSearch, i, 5)
      	SHA_Highlight_Border..Color		= dPixelColor(IMG_GradientRibbonSearch, i, 5)
      	Wait(0, waitMouseAndKeyboard)
      END
      
      // Forces a refresh 
      IF GR_RibbonSearch..Visible = False THEN
      	
      	// Hides the image and the superposition shape
      	IMG_Highlight_Background..Visible 	= False
      	SHA_Highlight_Border..Visible 	= False
      	
      	// Re-applies the initial pane image 
      	{gsNameRibbonControl, indControl}..Image 	= gsImageInitialPane
      	
      END
